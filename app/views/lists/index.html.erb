<div class="container">
    <h1>All Lists</h1>
  <div class="card-container">
    <% @lists.each do |list| %>
      <div class="list-card">
      <%= link_to list.name, list_path(list) %>
      </div>
    <% end %>
  </div>
    <%= link_to "Create a List", new_list_path,  class: "btn btn-primary btn-lg" %>
</div>
